void bubblesort(int[] a)
{
  int temp;
  for(int i=1,i<a.length;i++)
  {
    for(int j=0;j<a.length-i;j++)
    {
      if(a[j]>a[j+i])
      {
        temp=a[j];
        a[j]=a[j+i];
        a[j+i]=temp;
      }
    }
    for(int k=0;k<a.length;k++)
    {
      System.out.print(""+a[k]);
    }
    System.out.print("\n");
  }
}
