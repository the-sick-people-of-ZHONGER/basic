import java.util.Scanner;
public class p3_3
{
  static lomg fact(int n)
  {
    if(n<=1)
      return 1;
    else
      return n*fact(n-1);
  }
  public static void main(Srting[] args)
  {
    int i;
    Scanner input=new Scanner(System.in)
    i=input.nextInt();
    System.out.print(i+"answer is"+fact(i));
  }
}
